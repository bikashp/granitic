{
  "packages": [
    "github.com/graniticio/granitic/ws",
    "some/package/logic"
  ],
  "templates": {

    "PrivateHandler": {
      "ct": "Handler",
      "RequireAuthentication": true
    },

    "PublicHandler": {
      "compTemplate": "Handler",
      "RequireAuthentication": false
    },

    "Handler":{
      "t": "ws.WsHandler"
    }
  },

  "components": {

    "LoginHandler": {
      "compTemplate": "PublicHandler",
      "PasswordManager": "ref:PasswordManager",
      "SavePassword": false
    },

    "AccountHandler": {
      "compTemplate": "PrivateHandler",
      "SessionTimeoutSeconds": 10

    },

    "PasswordManager": {
      "t": "logic.PassMan",
      "Seed": 1.091223
    }

  }
}