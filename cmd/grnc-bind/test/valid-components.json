{
  "packages": [
    "github.com/graniticio/granitic/ws",
    "some/package/logic"
  ],
  "templates": {

    "PrivateHandler": {
      "ct": "Handler",
      "RequireAuthentication": true
    },

    "PublicHandler": {
      "compTemplate": "Handler",
      "RequireAuthentication": false
    },

    "Handler":{
      "t": "ws.WsHandler",
      "HTTPMethod": "POST"
    }
  },

  "components": {

    "LoginHandler": {
      "compTemplate": "PublicHandler",
      "PasswordManager": "ref:AuthManager",
      "SavePassword": false,
      "LoginBanner": "c:messages.login.welcome",
      "HTTPMethod": "GET"
    },

    "AccountHandler": {
      "compTemplate": "PrivateHandler",
      "SessionTimeoutSeconds": 10
    },

    "AuthManager": {
      "t": "logic.PassMan",
      "Seed": 1.091223,
      "DataSourceURL": "conf:externals.passwords"
    },

    "TypeTester": {
      "t": "logic.TypesTarget",
      "IntArray": [0,1,2],
      "FloatArray": [0.1,1.0,2.0],
      "StringArray": ["0", "1", "2"],
      "MixedArray": [true, 9, "test"],
      "Map": {
        "a": "b"
      },
      "ArrayMap": {
        "a": [0,1,2]
      },
      "StringArrayMap": {
        "a": ["0","1","2"]
      },
      "InterfaceArrayMap": {
        "a": ["0",1,true]
      }
    }

  }
}