{
  "RuntimeCtl": {
    "Server":{
      "Port": 9099,
      "AccessLogging": false,
      "TooBusyStatus": 503,
      "AutoFindHandlers": false,
      "MaxConcurrent": 1,
      "Address": "127.0.0.1"
    },
    "ResponseWriter": {
      "DefaultHeaders": {
        "Content-Type": "application/json; charset=utf-8"
      }
    },
    "Marshal": {
      "PrettyPrint": false,
      "IndentString": "  ",
      "PrefixString": ""
    },
    "ResponseWrapper": {
      "ErrorsFieldName": "Errors",
      "BodyFieldName":   "Response"
    },
    "CommandHandler": {
      "HttpMethod": "POST",
      "PathMatchPattern": "^/command$"
    }
  }
}